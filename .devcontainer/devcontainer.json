{
  "name": "TinyEMU Environment",
  "build": {
    "dockerfile": "../Dockerfile"
  },
  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "postCreateCommand": "make -C /tinyemu",
  "forwardPorts": [8000],
